{
  "name": "ory-editor",
  "version": "0.0.0",
  "description": "The next generation platform for creating well-defined, responsive, digital content - as easy as social media.",
  "main": "dist/bundle.js",
  "directories": {
    "doc": "./docs",
    "lib": "./dist"
  },
  "scripts": {
    "flow": "flow check --show-all-errors",
    "flow:watch": "npm run watch:js -- -c \"flow check --show-all-errors\"",
    "test": "npm-run-all test:unit:coverage test:e2e",
    "test:unit": "cross-env NODE_PATH=. jest",
    "test:unit:coverage": "npm run test:unit -- --coverage",
    "test:update": "cross-env NODE_PATH=. jest -u",
    "test:e2e": "node bin/nightwatch",
    "test:watch": "cross-env NODE_PATH=. jest --watch",
    "start": "npm run serve:home",
    "lint": "eslint \"+(src|test|types|examples)/**/*.js\" --fix",
    "lint:watch": "npm run watch:js -- -c \"npm run lint\"",
    "prebuild": "npm run clean",
    "watch:js": "chokidar \"+(examples|src|test|types)/**/*.js\" --initial",
    "docs": "documentation build ./src/editor/**/*.js --format md --github -o docs/API.md",
    "docs:watch": "documentation serve ./src/editor/**/*.js --watch",
    "heroku-postbuild": "npm run build:home",
    "clean": "rimraf \"dist/*\" \"lib/*\"",
    "build": "npm-run-all --parallel build:umd:min build:umd build:lib",
    "build:lib": "cross-env NODE_PATH=. babel src --out-dir lib",
    "build:umd:min": "cross-env NODE_PATH=. NODE_ENV=production webpack",
    "build:umd": "cross-env NODE_PATH=. webpack",
    "dev": "npm run dev:home",
    "dev:home": "cross-env NODE_PATH=. node examples/home/server.js",
    "dev:news-article": "cross-env NODE_PATH=. node examples/news-article/server.js",
    "dev:single-page-site": "cross-env NODE_PATH=. node examples/single-page-site/server.js",
    "build:home": "cross-env NODE_PATH=. node examples/home/build.js",
    "build:news-article": "cross-env NODE_PATH=. node examples/news-article/build.js",
    "build:single-page-site": "cross-env NODE_PATH=. node examples/single-page-site/build.js",
    "serve:home": "pushstate-server examples/home/build",
    "serve:news-article": "pushstate-server examples/news-article/build",
    "serve:single-page-site": "pushstate-server examples/single-page-site/build",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": "~3.8.0"
  },
  "author": "ORY GmbH",
  "license": "AGPL-3.0",
  "peerDependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-tap-event-plugin": "^2.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "deep-equal": "^1.0.1",
    "device.js": "^0.1.2",
    "dnd-core": "^2.0.2",
    "element-resize-event": "^2.0.4",
    "katex": "^0.6.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "material-ui": "^0.16.3",
    "mousetrap": "^1.6.0",
    "ory-redux-undo": "^0.0.1",
    "ramda": "^0.23.0",
    "react": "^15.4.1",
    "react-css-modules": "^4.1.0",
    "react-dimensions": "^1.3.0",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.4.1",
    "react-draggable": "^2.2.3",
    "react-hotkeys": "^0.9.0",
    "react-katex": "^1.0.1",
    "react-player": "^0.14.0",
    "react-portal": "^3.0.0",
    "react-redux": "^5.0.1",
    "react-resizable": "^1.4.5",
    "react-tap-event-plugin": "^2.0.0",
    "redux": "^3.6.0",
    "reselect": "^2.5.4",
    "selection-position": "^1.0.0",
    "semver": "^5.3.0",
    "slate": "^0.16.8",
    "slate-edit-blockquote": "^0.2.0",
    "slate-edit-list": "^0.5.3",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "chokidar-cli": "^1.2.0",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "deep-equal": "^1.0.1",
    "detect-port": "^1.0.6",
    "documentation": "4.0.0-beta15",
    "dotenv": "^2.0.0",
    "eslint": "^3.11.1",
    "eslint-config-ory-am": "^3.0.0",
    "eslint-config-ory-am-react": "^3.0.0",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "filesize": "^3.3.0",
    "find-cache-dir": "^0.1.1",
    "flow-bin": "^0.37.4",
    "fs-extra": "^1.0.0",
    "gzip-size": "^3.0.0",
    "html-webpack-plugin": "^2.24.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^18.1.0",
    "json-loader": "^0.5.4",
    "nightwatch": "^0.9.8",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.0",
    "postcss-css-variables": "^0.6.0",
    "postcss-loader": "^1.2.0",
    "promise": "^7.1.1",
    "pushstate-server": "^2.1.0",
    "raven-js": "^3.8.0",
    "react-addons-perf": "^15.4.1",
    "react-addons-shallow-compare": "^15.4.1",
    "react-dev-utils": "^0.4.0",
    "react-test-renderer": "^15.4.1",
    "recursive-readdir": "^2.1.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "unexpected": "^10.20.0",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-server": "2.1.0-beta.11",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^.*\\.scoped\\.css$": "identity-obj-proxy",
      "^.+\\.css$": "<rootDir>/config/jest/CSSStub.js"
    }
  }
}
